                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="NextButtonStyle" TargetType="{x:Type Button}">

        <Setter Property="Background" Value="{StaticResource BaseBackgroundColor}" />
        <Setter Property="BorderBrush" Value="#BABABA" />
        <Setter Property="BorderThickness" Value="0.5" />
        <Setter Property="Foreground" Value="#333333" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Name="NextButtonBorder"
                            Background="{TemplateBinding Background}"
                            BorderBrush="#BABABA"
                            BorderThickness="0.5">
                        <Grid>
                            <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">


                                <TextBlock x:Name="NextText"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Foreground="{TemplateBinding Foreground}"
                                           Text="Next" />
                                <Path Name="NextPath"
                                      Width="16"
