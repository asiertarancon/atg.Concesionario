    <Style x:Key="PreviousButtonStyle" TargetType="{x:Type Button}">

        <Setter Property="Background" Value="{StaticResource BaseBackgroundColor}" />
        <Setter Property="BorderBrush" Value="#BABABA" />
        <Setter Property="BorderThickness" Value="0.5" />
        <Setter Property="Foreground" Value="#333333" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border Name="PreviousButtonBorder"
                            Background="{TemplateBinding Background}"
                            BorderBrush="#BABABA"
                            BorderThickness="0.5">
                        <Grid>
                            <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                                <Path Name="PreviousPath"
                                      Width="16"
                                      Height="16"
                                      Margin="0,0,5,0"
                                      Data="F1M1928.8,1024.07L1920.05,1024.07 1905.62,1009.64 1901.24,1005.26 1920.1,986.403 1928.78,986.403 1928.82,986.439 1913.09,1002.17 1954.58,1002.17 1954.58,1008.35 1913.09,1008.35 1928.8,1024.07z"
                                      Fill="#333333"
                                      RenderTransformOrigin="0.5,0.5"
                                      Stretch="Uniform">
                                    <Path.RenderTransform>
                                        <TransformGroup>
                                            <TransformGroup.Children>
                                                <RotateTransform Angle="0" />
                                                <ScaleTransform ScaleX="1" ScaleY="1" />
                                            </TransformGroup.Children>
                                        </TransformGroup>
                                    </Path.RenderTransform>
                                </Path>

                                <TextBlock x:Name="PreviousText"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           Foreground="{TemplateBinding Foreground}"
                                           Text="Previous" />
                            </StackPanel>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="PreviousButtonBorder" Property="Background" Value="{StaticResource GridHeaderCellBackground}" />
                            <Setter TargetName="PreviousPath" Property="Fill" Value="White" />
                            <Setter TargetName="PreviousText" Property="Foreground" Value="White" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="PreviousButtonBorder" Property="Opacity" Value="0.5" />
